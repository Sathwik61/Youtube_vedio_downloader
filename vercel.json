{
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "ytdownloader/wsgi.py"
      }
    ],
    "scripts": {
      "build": "./build_files.sh"
    }
  }